<%- include('includes/header') %>
      <div class="row">
        <%- include('includes/sidemenu') %>
        <div class="col-lg-9">
            <% if(msg!="") {  %>
                <div class="alert alert-primary" role="alert">
                    <strong><%= msg %></strong>
                </div>
                <% }  %>
            <form action="/users/addmoney" method="post">
                <div class="form-group">
                  <label for="">Phone</label>
                  <input type="text" value="<%= result1[0].phone %>" readonly
                    class="form-control" name="phone" id="" aria-describedby="helpId" placeholder="">
                  <small id="helpId" class="form-text text-muted">*</small>
                </div>
                <div class="form-group">
                    <label for="">Amount</label>
                    <input type="number" required
                      class="form-control" name="amount" id="" aria-describedby="helpId" placeholder="">
                    <small id="helpId" class="form-text text-muted">*</small>
                  </div>
                  <div class="form-group">
                    <label for="">Acc</label>
                    <input type="number " required
                      class="form-control" name="acc" id="" aria-describedby="helpId" placeholder="">
                    <small id="helpId" class="form-text text-muted">*</small>
                  </div>
                  <div class="form-group">
                    <label for="">cvv</label>
                    <input type="password" required
                      class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
                    <small id="helpId" class="form-text text-muted">*</small>
                  </div>
                  <button type="submit" class="btn btn-primary">Add Money</button>
            </form>
        </div>
      </div>
      <%- include('includes/footer') %>